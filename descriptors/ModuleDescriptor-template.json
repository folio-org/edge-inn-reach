{
  "id": "@artifactId@-@version@",
  "name": "INN-REACH system Edge API module",
  "provides": [],
  "requires": [
    {
      "id": "login",
      "version": "7.3"
    },
    {
      "id": "users",
      "version": "16.1"
    },
    {
      "id": "permissions",
      "version": "5.6"
    }
  ],
  "permissionSets" : [],
  "launchDescriptor": {
    "dockerImage": "@artifactId@:@version@",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 268435456,
        "PortBindings": { "8081/tcp": [ { "HostPort": "%p" } ] }
      }
    },
    "env": [
      { "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      }
    ]
  }
}
